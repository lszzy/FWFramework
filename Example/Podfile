source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '11.0'
use_frameworks!

target "Example" do
  project 'FWFramework', 'Testing' => :release, 'Staging' => :release

  pod 'FWFramework', :path => '../', :subspecs => ['FWFramework', 'Compatible', 'Contacts', 'Microphone', 'Calendar', 'AppleMusic', 'Tracking']
  pod 'FWDebug', :configurations => ['Debug', 'Testing']
end
