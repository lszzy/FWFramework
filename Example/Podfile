platform :ios, '9.0'
use_frameworks!

target "Example" do
  pod 'FWFramework', :path => '../'
  pod 'FWDebug', :configurations => ['Debug']
  pod 'Mediator', :path => 'Mediator/Mediator/'
  pod 'User', :path => 'Mediator/User/'
  pod 'Test', :path => 'Mediator/Test/'
end

target "AppClip" do
  pod 'FWFramework', :path => '../', :subspecs => ['Application/Database', 'Application/Image', 'Application/Request', 'Component/SwiftUI']
  pod 'FWDebug', :configurations => ['Debug']
end
