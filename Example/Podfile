
target "Example" do
  platform :ios, '9.0'
  use_frameworks!

  pod 'FWFramework', :path => '../'
  pod 'FWDebug', :configurations => ['Debug']
end

target "AppClip" do
  platform :ios, '14.0'
  use_frameworks!

  pod 'FWFramework', :path => '../', :subspecs => ['Framework', 'Component']
  pod 'FWDebug', :configurations => ['Debug']
end
