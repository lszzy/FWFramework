platform :ios, '8.0'
use_frameworks!

target "Example2" do
    pod 'FWFramework', :path => '../', :subspecs => ['Framework', 'Application', 'Image-Webp', 'Authorize-Contacts', 'Authorize-Microphone', 'Authorize-Calendar', 'Authorize-AppleMusic']
    pod 'FWDebug', :configurations => ['Debug']

    flutter_application_path = 'Flutter/example'
    load File.join(flutter_application_path, '.ios', 'Flutter', 'podhelper.rb')
    install_all_flutter_pods(flutter_application_path)
end

